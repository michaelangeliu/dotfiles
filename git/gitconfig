[alias]
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	s = status --short --branch
	co = checkout
	br = branch
	wip = !git add -A && git commit -m \"wip: DO NOT PUSH\" --no-verify
	rh1 = reset HEAD~1
[core]
	editor = code --wait
[init]
	defaultBranch = main
[sequence]
	editor = code --wait --reuse-window
[rerere]
	enabled = true
[user]
	name = Michael Liu
